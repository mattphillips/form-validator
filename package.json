{
  "name": "es6-prototype-boilerplate",
  "version": "0.0.1",
  "description": "Quick prototying setup for es6 projects",
  "main": "index.js",
  "scripts": {
    "start": "budo index.js -p 8080 --live -- -t [ babelify --presets [ es2015 stage-1 ] ] ",
    "test": "mocha **/*.spec.js --compilers js:babel-register",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- **/*.spec.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ]
  },
  "author": "Matt Phillips",
  "license": "ISC",
  "devDependencies": {
    "babelify": "^7.3.0",
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-1": "^6.5.0",
    "budo": "^8.3.0",
    "chai": "^3.5.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3"
  }
}
